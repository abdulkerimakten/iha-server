# Pixhawk connection (choose one):
# udp:127.0.0.1:14550  or  /dev/ttyTHS1,57600  or  /dev/ttyACM0,115200
VEHICLE_CONN=udp:127.0.0.1:14550
# Takeoff altitude (meters)
TAKEOFF_ALT=20
# YOLO model path (.pt / .onnx / .engine)
YOLO_MODEL=models/plate_yolov8n.pt
# Camera source (device path or RTSP URL)
CAMERA_SOURCE=/dev/video0
# Minimum confidence to accept detection
MIN_CONF=0.6
# Stable frames required before sending target
STABLE_N=5
# UDP target for IDA/GCS
IDA_HOST=192.168.1.50
IDA_PORT=5005
# Color class names in the trained model (comma separated)
COLOR_CLASSES=red,green,black
# If model outputs a single 'plate' class, enable HSV color estimation
HSV_FALLBACK=true
# Mirror target info to telemetry as STATUSTEXT (no file writes)
MIRROR_STATUSTEXT=true