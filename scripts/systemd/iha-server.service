[Unit]
Description=IHA Server (Jetson)
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
WorkingDirectory=%h/iha-server
Environment=PYTHONUNBUFFERED=1
ExecStart=%h/iha-server/venv/bin/python -m iha_server.main
Restart=on-failure
RestartSec=3

[Install]
WantedBy=multi-user.target